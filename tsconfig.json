{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es5"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "dist",
    "sourceMap": true,
    // "noEmitOnError":true
  }
}